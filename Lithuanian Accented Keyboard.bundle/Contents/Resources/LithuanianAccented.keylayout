    <keyMapSet id="ANSI">
        <keyMap index="0" baseMapSet="ISO" baseIndex="0">
            <!-- Level 1: no modifiers -->
            <!-- Key 50 has position B00 on ISO keyboards, but E00 on ANSI keyboards -->
            <key code="50" action="grave"/> <!-- deadkey grave -->
        </keyMap>
        <keyMap index="1" baseMapSet="ISO" baseIndex="1">
            <!-- Level 2: Shift pressed -->
            <!-- Key 50 has position B00 on ISO keyboards, but E00 on ANSI keyboards -->
            <key code="50" action="tilde"/> <!-- deadkey tilde -->
        </keyMap>
        <keyMap index="2" baseMapSet="ISO" baseIndex="2">
            <!-- Caps Lock on: same as keyMap 1 for letters, same as keyMap 0 for other symbols -->
            <!-- Key 50 has position B00 on ISO keyboards, but E00 on ANSI keyboards -->
            <key code="50" action="grave"/> <!-- deadkey grave -->
        </keyMap>
        <keyMap index="3" baseMapSet="ISO" baseIndex="3">
            <!-- Level 3: Option pressed (Macs don't have an AltGr key) -->
            <!-- There is no key in position B00 on ANSI keyboards. Compensate it with Option level of keys B01, B02, B03-->
            <key code="6" output="&#x003c;"/> <!-- LESS-THAN SIGN -->
            <key code="7" output="&#x003e;"/> <!-- GREATER-THAN SIGN -->
            <key code="8" output="&#x2013;"/> <!-- EN DASH -->
            <!-- Key 50 has position B00 on ISO keyboards, but E00 on ANSI keyboards -->
            <key code="50" action="acute"/> <!-- deadkey acute -->
        </keyMap>
    </keyMapSet>
    <keyMapSet id="JIS">
        <keyMap index="0" baseMapSet="ISO" baseIndex="0">
            <!-- Level 1: no modifiers -->
            <!-- Key 93 has position E13 on JIS keyboards, and doesn't exist elsewhere. Use it to compensate missing E00 key -->
            <key code="93" action="grave"/>
            <!-- deadkey grave -->
            <!-- Key 94 has position B11 on JIS keyboards, and doesn't exist elsewhere. Use it to compensate missing B00 key -->
            <key code="94" output="&#x003c;"/>
            <!-- LESS-THAN SIGN -->
            <!-- Key 95 is an additional digit separator on JIS keyboards -->
            <key code="95" output="."/>
        </keyMap>
        <keyMap index="1" baseMapSet="ISO" baseIndex="1">
            <!-- Level 2: Shift pressed -->
            <!-- Key 93 has position E13 on JIS keyboards, and doesn't exist elsewhere. Use it to compensate missing E00 key -->
            <key code="93" action="tilde"/>
            <!-- deadkey tilde -->
            <!-- Key 94 has position B11 on JIS keyboards, and doesn't exist elsewhere. Use it to compensate missing B00 key -->
            <key code="94" output="&#x003e;"/>
            <!-- GREATER-THAN SIGN -->
            <!-- Key 95 is an additional digit separator on JIS keyboards -->
            <key code="95" output="."/>
        </keyMap>
        <keyMap index="2" baseMapSet="ISO" baseIndex="2">
            <!-- Caps Lock on: same as keyMap 1 for letters, same as keyMap 0 for other symbols -->
            <!-- Key 93 has position E13 on JIS keyboards, and doesn't exist elsewhere. Use it to compensate missing E00 key -->
            <key code="93" action="grave"/>
            <!-- deadkey grave -->
            <!-- Key 94 has position B11 on JIS keyboards, and doesn't exist elsewhere. Use it to compensate missing B00 key -->
            <key code="94" output="&#x003c;"/>
            <!-- LESS-THAN SIGN -->
            <!-- Key 95 is an additional digit separator on JIS keyboards -->
            <key code="95" output="."/>
        </keyMap>
        <keyMap index="3" baseMapSet="ISO" baseIndex="3">
            <!-- Level 3: Option pressed (Macs don't have an AltGr key) -->
            <!-- There is no key in position B00 on JIS keyboards. Compensate it with Option level of keys B01, B02, B03 for compatibility with ANSI -->
            <key code="6" output="&#x003c;"/>
            <!-- LESS-THAN SIGN -->
            <key code="7" output="&#x003e;"/>
            <!-- GREATER-THAN SIGN -->
            <key code="8" output="&#x2013;"/>
            <!-- EN DASH -->
            <!-- Key 93 has position E13 on JIS keyboards, and doesn't exist elsewhere. Use it to compensate missing E00 key -->
            <key code="93" action="acute"/>
            <!-- deadkey acute -->
            <!-- Key 94 has position B11 on JIS keyboards, and doesn't exist elsewhere. Use it to compensate missing B00 key -->
            <key code="94" output="&#x2013;"/>
            <!-- EN DASH -->
            <!-- Key 95 is an additional digit separator on JIS keyboards -->
            <key code="95" output="."/>
        </keyMap>
    </keyMapSet>
