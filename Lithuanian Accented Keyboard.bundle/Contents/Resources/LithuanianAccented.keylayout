            <when state="grave" output="&#x1e80;"/> <!-- LATIN CAPITAL LETTER W WITH GRAVE -->
        </action>
        <action id="w">
            <when state="none" output="w"/>
            <when state="acute" output="&#x1e83;"/> <!-- LATIN SMALL LETTER W WITH ACUTE -->
            <when state="circumflex" output="&#x0175;"/> <!-- LATIN SMALL LETTER W WITH CIRCUMFLEX -->
            <when state="diaeresis" output="&#x1e85;"/> <!-- LATIN SMALL LETTER W WITH DIAERESIS -->
            <when state="grave" output="&#x1e81;"/> <!-- LATIN SMALL LETTER W WITH GRAVE -->
        </action>
        <action id="X">
            <when state="none" output="X"/>
            <when state="diaeresis" output="&#x1e8c;"/> <!-- LATIN CAPITAL LETTER X WITH DIAERESIS -->
        </action>
        <action id="x">
            <when state="none" output="x"/>
            <when state="diaeresis" output="&#x1e8d;"/> <!-- LATIN SMALL LETTER X WITH DIAERESIS -->
        </action>
        <action id="Y">
            <when state="none" output="Y"/>
            <when state="acute" output="&#x00dd;"/> <!-- LATIN CAPITAL LETTER Y WITH ACUTE -->
            <when state="circumflex" output="&#x0176;"/> <!-- LATIN CAPITAL LETTER Y WITH CIRCUMFLEX -->
            <when state="diaeresis" output="&#x0178;"/> <!-- LATIN CAPITAL LETTER Y WITH DIAERESIS -->
            <when state="grave" output="&#x1ef2;"/> <!-- LATIN CAPITAL LETTER Y WITH GRAVE -->
            <when state="tilde" output="&#x1ef8;"/> <!-- LATIN CAPITAL LETTER Y WITH TILDE -->
        </action>
        <action id="y">
            <when state="none" output="y"/>
            <when state="acute" output="&#x00fd;"/> <!-- LATIN SMALL LETTER Y WITH ACUTE -->
            <when state="circumflex" output="&#x0177;"/> <!-- LATIN SMALL LETTER Y WITH CIRCUMFLEX -->
            <when state="diaeresis" output="&#x00ff;"/> <!-- LATIN SMALL LETTER Y WITH DIAERESIS -->
            <when state="grave" output="&#x1ef3;"/> <!-- LATIN SMALL LETTER Y WITH GRAVE -->
            <when state="tilde" output="&#x1ef9;"/> <!-- LATIN SMALL LETTER Y WITH TILDE -->
        </action>
        <action id="Z">
            <when state="none" output="Z"/>
            <when state="acute" output="&#x0179;"/> <!-- LATIN SMALL LETTER Z WITH ACUTE -->
            <when state="diaeresis" output="&#x1e90;"/> <!-- LATIN SMALL LETTER Z WITH CIRCUMFLEX -->
        </action>
        <action id="z">
            <when state="none" output="z"/>
            <when state="acute" output="&#x017a;"/> <!-- LATIN SMALL LETTER Z WITH ACUTE -->
            <when state="diaeresis" output="&#x1e91;"/> <!-- LATIN SMALL LETTER Z WITH CIRCUMFLEX -->
        </action>
        <action id="acute">
            <when state="none" next="acute"/>
            <when state="acute" output="&#x0301;"/> <!-- COMBINING ACUTE ACCENT -->
        </action>
        <action id="circumflex">
            <when state="none" next="circumflex"/>
            <when state="circumflex" output="&#x0302;"/> <!-- COMBINING CIRCUMFLEX -->
        </action>
        <action id="diaeresis">
            <when state="none" next="diaeresis"/>
            <when state="diaeresis" output="&#x0308;"/> <!-- COMBINING DIAERESIS -->
        </action>
        <action id="tilde">
            <when state="none" next="tilde"/>
            <when state="tilde" output="&#x0303;"/> <!-- COMBINING TILDE -->
        </action>
        <action id="grave">
            <when state="none" next="grave"/>
            <when state="acute" output="&#x0301;"/> <!-- COMBINING ACUTE ACCENT -->
            <when state="circumflex" output="&#x0302;"/> <!-- COMBINING CIRCUMFLEX -->
            <when state="diaeresis" output="&#x0308;"/> <!-- COMBINING DIAERESIS -->
            <when state="grave" output="&#x0300;"/> <!-- COMBINING GRAVE ACCENT -->
            <when state="tilde" output="&#x0303;"/> <!-- COMBINING TILDE -->
        </action>
        <action id=".">
            <when state="none" output="."/>
            <when state="grave" output="&#x0307;"/> <!-- COMBINING DOT ABOVE -->
        </action>
    </actions>
    <terminators>
        <when state="acute" output="&#x00b4;"/> <!-- ACUTE ACCENT -->
        <when state="circumflex" output="^"/>
        <when state="diaeresis" output="&#x0022;"/> <!-- QUOTATION MARK -->
        <when state="grave" output="`"/>
        <when state="tilde" output="~"/>
    </terminators>
</keyboard>
